{
  "name": "react-version",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,md,css,scss,html}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "yarn test --watchAll=false --passWithNoTests --findRelatedTests"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "10.0.2",
    "@testing-library/user-event": "10.0.1",
    "node-sass": "4.13.1",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.0",
    "wenk": "1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "4.2.3",
    "lint-staged": "10.1.3",
    "prettier": "2.0.4"
  }
}
