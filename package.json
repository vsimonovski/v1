{
  "name": "react-version",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,scss,html}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "yarn test --watchAll=false"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.1",
    "@testing-library/user-event": "10.0.0",
    "node-sass": "4.13.1",
    "normalize.css": "8.0.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-scripts": "3.4.0",
    "styled-components": "^5.0.1",
    "wenk": "1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1"
  }
}
