{
  "name": "react-version",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,md,css,scss,html}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "yarn test --watchAll=false --passWithNoTests --findRelatedTests"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "10.4.9",
    "@testing-library/user-event": "12.2.2",
    "node-sass": "5.0.0",
    "normalize.css": "8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.1",
    "styled-components": "5.2.1",
    "wenk": "1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "prettier": "2.2.0"
  }
}
